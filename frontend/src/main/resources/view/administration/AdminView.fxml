<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="663.0" prefWidth="708.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8.0.162-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="bbk_beam.mtRooms.ui.controller.administration.AdministrationController">
   <children>
       <TabPane fx:id="admin_TabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE"
                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="0.0">
           <tabs>
               <Tab fx:id="userAccount_Tab" text="User Accounts">
                   <content>
                       <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                               <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                         AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                   <columnConstraints>
                                       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                   </columnConstraints>
                                   <rowConstraints>
                                       <RowConstraints maxHeight="-Infinity" minHeight="40.0" prefHeight="40.0"
                                                       vgrow="SOMETIMES"/>
                                       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                   </rowConstraints>
                                   <children>
                                       <TableView fx:id="account_table" stylesheets="@../../css/MtRoomsGUI.css"
                                                  GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                                                  GridPane.rowIndex="1" GridPane.valignment="TOP"
                                                  GridPane.vgrow="ALWAYS">
                                           <columns>
                                               <TableColumn fx:id="id_col" maxWidth="1.7976931348623157E308"
                                                            prefWidth="53.0" text="id"/>
                                               <TableColumn fx:id="username_col" prefWidth="119.0" text="username"/>
                                               <TableColumn fx:id="created_col" prefWidth="131.0" text="created"/>
                                               <TableColumn fx:id="login_col" minWidth="0.0" prefWidth="111.0"
                                                            text="last login"/>
                                               <TableColumn fx:id="pwd_change_col" prefWidth="155.0"
                                                            text="last pwd change"/>
                                               <TableColumn fx:id="type_col" minWidth="0.0" prefWidth="107.0"
                                                            text="type"/>
                                               <TableColumn fx:id="active_col" prefWidth="64.0" text="active"/>
                                           </columns>
                                       </TableView>
                                       <ToolBar prefHeight="40.0" prefWidth="200.0" GridPane.hgrow="ALWAYS">
                                           <items>
                                               <Button fx:id="newAccount_Button" mnemonicParsing="false"
                                                       onAction="#handleNewAccountAction" text="New"/>
                                               <Button fx:id="editAccount_Button" mnemonicParsing="false"
                                                       onAction="#handleEditAccountAction" text="Edit"/>
                                           </items>
                                       </ToolBar>
                                   </children>
                               </GridPane>
                           </children>
                       </AnchorPane>
                   </content>
               </Tab>
               <Tab fx:id="membership_Tab" text="Memberships &amp; Discounts">
                   <content>
                       <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                               <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                         AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                   <columnConstraints>
                                       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                   </columnConstraints>
                                   <rowConstraints>
                                       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                   </rowConstraints>
                               </GridPane>
                           </children>
                       </AnchorPane>
                   </content>
               </Tab>
               <Tab fx:id="inventory_Tab" text="Inventory">
                   <content>
                       <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                               <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                     AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                     AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                   <children>
                                       <GridPane maxHeight="1.7976931348623157E308" maxWidth="400.0"
                                                 HBox.hgrow="ALWAYS">
                                           <columnConstraints>
                                               <ColumnConstraints hgrow="SOMETIMES" maxWidth="400.0" minWidth="10.0"
                                                                  prefWidth="200.0"/>
                                           </columnConstraints>
                                           <rowConstraints>
                                               <RowConstraints maxHeight="-Infinity" minHeight="40.0" prefHeight="40.0"
                                                               vgrow="SOMETIMES"/>
                                               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                           </rowConstraints>
                                           <children>
                                               <HBox prefHeight="100.0" prefWidth="200.0">
                                                   <children>
                                                       <MenuButton fx:id="newInventory_MenuButton"
                                                                   mnemonicParsing="false" prefHeight="26.0"
                                                                   prefWidth="80.0" text="%Button_AddGeneric">
                                                           <items>
                                                               <MenuItem fx:id="newBuilding_MenuItem"
                                                                         mnemonicParsing="false"
                                                                         onAction="#handleAddBuildingAction"
                                                                         text="%Button_AddBuilding"/>
                                                               <MenuItem fx:id="newInventory_MenuItem"
                                                                         mnemonicParsing="false"
                                                                         onAction="#handleAddInventoryAction"
                                                                         text="Add generic" visible="false"/>
                                                           </items>
                                                           <HBox.margin>
                                                               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                                           </HBox.margin>
                                                       </MenuButton>
                                                       <Button fx:id="editInventory_Button" disable="true"
                                                               mnemonicParsing="false"
                                                               onAction="#handleEditInventoryAction" prefHeight="26.0"
                                                               prefWidth="80.0" text="%Button_EditGeneric">
                                                           <HBox.margin>
                                                               <Insets bottom="5.0" right="5.0" top="5.0"/>
                                                           </HBox.margin>
                                                       </Button>
                                                       <Button fx:id="deleteInventory_Button" disable="true"
                                                               mnemonicParsing="false"
                                                               onAction="#handleDeleteInventoryAction" prefHeight="26.0"
                                                               prefWidth="80.0" text="%Button_RemoveGeneric">
                                                           <HBox.margin>
                                                               <Insets bottom="5.0" right="5.0" top="5.0"/>
                                                           </HBox.margin>
                                                       </Button>
                                                   </children>
                                               </HBox>
                                               <TreeView fx:id="inventory_TreeView" maxWidth="400.0"
                                                         GridPane.rowIndex="1"/>
                                           </children>
                                       </GridPane>
                                       <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                                           <columnConstraints>
                                               <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"
                                                                  minWidth="200.0" prefWidth="400.0"/>
                                           </columnConstraints>
                                           <rowConstraints>
                                               <RowConstraints maxHeight="-Infinity" minHeight="40.0" prefHeight="40.0"
                                                               vgrow="SOMETIMES"/>
                                               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                           </rowConstraints>
                                           <children>
                                               <HBox prefHeight="100.0" prefWidth="200.0"/>
                                               <TextArea fx:id="inventoryDescription_TextArea"
                                                         maxHeight="1.7976931348623157E308"
                                                         maxWidth="1.7976931348623157E308" GridPane.rowIndex="1">
                                                   <GridPane.margin>
                                                       <Insets left="5.0" right="5.0"/>
                                                   </GridPane.margin>
                                               </TextArea>
                                           </children>
                                       </GridPane>
                                   </children>
                                   <padding>
                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                   </padding>
                               </HBox>
                           </children>
                       </AnchorPane>
                   </content>
               </Tab>
           </tabs>
       </TabPane>
   </children>
</AnchorPane>
